# http://editorconfig.org
root = true

[*]
charset = utf-8
end_of_line = lf
indent_size = 2
indent_style = space
insert_final_newline = true
# editorconfig-tools is unable to ignore longs strings or urls
max_line_length = off
trim_trailing_whitespace = true
max_line_length = 80

[*.md]
indent_size = false
